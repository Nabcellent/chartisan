{
    "name": "nabcellent/chartisan",
    "description": "The laravel charting package",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/nabcellent/chartisan",
    "keywords": [
        "library",
        "chartisan",
        "charts"
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/console": "^8.0|^9.0",
        "balping/json-raw-encoder": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.4"
    },
    "authors": [
        {
            "role": "De veloper",
            "name": "Nabcellent",
            "email": "nabcellent.dev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nabcellent\\Charts\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nabcellent\\Charts\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
