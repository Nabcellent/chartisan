{
    "name": "nabcellent/chartisan",
    "description": "The laravel charting package",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/nabcellent/chartisan",
    "keywords": [
        "library",
        "chartisan",
        "charts"
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/console": "^9.48",
        "illuminate/contracts": "^9.48",
        "illuminate/http": "^9.48",
        "illuminate/support": "^9.48"
    },
    "require-dev": {
        "laravel/pint": "^1.4",
        "phpstan/phpstan": "^1.9"
    },
    "authors": [
        {
            "role": "De veloper",
            "name": "Nabcellent",
            "email": "nabcellent.dev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nabcellent\\Chartisan\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nabcellent\\Chartisan\\ChartisanServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
